package PDV;

/**
 *
 * @author LucasSilvaGarcia
 */
public class Produtos {

    public Produtos() {
    }
    String NomeProduto;
    int Quantidade;
    float Preco;

    public void getNomeProduto(String NomeProduto) {
        this.NomeProduto = NomeProduto;
    }

    public void getQuantidade(Integer Quantidade) {
        this.Quantidade = Quantidade;
    }

    public void getPreco(Float Preco) {
        this.Preco = Preco;
    }

    public String setNomeProduto() {
        return NomeProduto;
    }

    public Integer setQuantidade() {
        return Quantidade;
    }

    public float setPreco() {
        return Preco;
    }

}
