package PDV;

/**
 *
 * @author LucasSilvaGarcia
 */
public class Estoque {

    public Estoque() {
    }
    String Nome;
    int Quantidade;

    public void getNome(String Nome) {
        this.Nome = Nome;
    }

    public void getQuantidade(int Quantidade) {
        this.Quantidade = Quantidade;
    }

    public String setNome() {
        return Nome;
    }

    public Integer setQuantidade() {
        return Quantidade;
    }

}
